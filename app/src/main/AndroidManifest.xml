<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="ikhwan.binar.myjetpackcompose">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyJetpackCompose"
        tools:targetApi="31">
        <activity
            android:name=".latihan.MainLatihanActivity"
            android:exported="true"
            android:label="@string/title_activity_main_latihan"
            android:theme="@style/Theme.MyJetpackCompose">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".latihan.PaymentActivity"
            android:exported="true"
            android:label="@string/title_activity_payment"
            android:theme="@style/Theme.MyJetpackCompose"></activity>
        <activity
            android:name=".latihan.VolumeActivity"
            android:exported="true"
            android:label="@string/title_activity_volume"
            android:theme="@style/Theme.MyJetpackCompose"></activity>
        <activity
            android:name=".latihan.LoginActivity"
            android:exported="true"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.MyJetpackCompose"></activity>
        <activity
            android:name=".MainActivity2"
            android:exported="false"
            android:label="@string/title_activity_main2"
            android:theme="@style/Theme.MyJetpackCompose" />
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:label="@string/title_activity_splash"
            android:theme="@style/Theme.MyJetpackCompose" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyJetpackCompose">

        </activity>
    </application>

</manifest>